      "@nestjs/axios": "^3.0.0",
      "axios": "^1.4.0",
      "mysql2": "^3.2.0",
      "typeorm": "^0.3.17",
      "rxjs": "^7.8.1",
      "dotenv": "^16.3.1",
      "bull": "^4.11.0",
      "@nestjs/bull": "^3.0.2"
    },
    "devDependencies": {
      "@nestjs/cli": "^10.0.0",
      "@nestjs/schematics": "^10.0.0",
      "@nestjs/testing": "^10.0.0",
      "typescript": "^5.1.6",
      "eslint": "^8.45.0",
      "eslint-config-prettier": "^9.0.0",
      "eslint-plugin-prettier": "^5.0.0",
      "prettier": "^3.0.0",
      "jest": "^29.6.2",
      "@types/jest": "^29.5.3",
      "ts-jest": "^29.1.0",
      "supertest": "^6.3.3"
