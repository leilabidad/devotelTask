    "description": "A NestJS backend application for fetching, transforming, storing, and retrieving job offers.",
    "main": "dist/main.js",
    "scripts": {
      "start": "node dist/main.js",
      "start:dev": "nest start --watch",
      "build": "tsc",
      "lint": "eslint . --ext .ts",
